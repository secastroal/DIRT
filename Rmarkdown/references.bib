month = {dec},
pages = {975},
publisher = {Frontiers},
title = {{Analyzing latent state-trait and multiple-indicator latent growth curve models as multilevel structural equation models}},
url = {http://journal.frontiersin.org/article/10.3389/fpsyg.2013.00975/abstract},
volume = {4},
