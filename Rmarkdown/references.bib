publisher = {Academic Press},
title = {{Trait anxiety and trait anger measured by ecological momentary assessment and their correspondence with traditional trait questionnaires}},
url = {https://www.sciencedirect.com/science/article/pii/S0092656613001116},
volume = {47},
year = {2013}
