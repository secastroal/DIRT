issn = {15366359},
journal = {Measurement},
keywords = {Item response theory,MCMC,posterior predictive checks,posterior predictive data,prior specification},
month = {oct},
number = {4},
pages = {239--255},
publisher = {Taylor and Francis Inc.},
title = {{Prior Sensitivity of the Posterior Predictive Checks Method for Item Response Theory Models}},
volume = {16},
year = {2018}
}
@article{Janson2017,
abstract = {Background: Psychosocial stress represents an unavoidable and often long-lasting burden over the lifespan. While chronic exposure to stress is considered as antecedent of stress-related disease, designing interventions to promote healthy coping behavior requires a better understanding about the role of individual coping styles as well as the efficiency of state-dependent coping responses with regard to cortisol and alpha amylase recovery from acute psychosocial stress.

Methods: In order to evaluate the differential stress-buffering effect of coping measures, fifty-nine healthy adults (59.3{\%} female, Mage = 22.93 years) underwent the Trier Social Stress Test (TSST) and provided saliva samples before and repeatedly after stress. Coping patterns were obtained using trait and state coping questionnaires (SVF 78, SVF ak-42).

Results: Hierarchical linear modeling was used to test for relationships between coping measures and endocrine activity. Independent of sex, age, BMI, chronic stress, and depressiveness, cortisol recovery was predicted by state-dependent distraction coping (slope: $\beta$ = −0.0430, SE = 0.0184, p = 0.02), while alpha amylase recovery, on the contrary, was determined by defensive (slope: $\beta$ = −0.0233, SE = 0.0096, p = 0.02) and resigning coping styles (slope: $\beta$ = −0.0214, SE = 0.0093, p = 0.03).

Conclusions: The results reveal state coping as determinants of better cortisol recovery as well as coping styles as determinants of better alpha amylase recovery from psychosocial stress. To provide further evidence of this assumed health-promoting effect of coping, future investigations linking coping and inflammatory processes should shed further light on the relationship between cognitive coping and health.},
author = {Janson, Johanna and Rohleder, Nicolas},
doi = {10.1016/J.PSYNEUEN.2017.07.275},
journal = {Psychoneuroendocrinology},
month = {sep},
pages = {14},
publisher = {Pergamon},
title = {{State and trait coping is differentially linked to cortisol and alpha amylase recovery from acute psychosocial stress}},
url = {https://www.sciencedirect.com/science/article/pii/S0306453017309204},
volume = {83},
year = {2017}
}
