@article{Suchman2016,
abstract = {This paper reports on a data-driven effort to map the temporal and conceptual relationships among a range of entrepreneurial start-up activities -- developing a business model, incorporating, registering intellectual property, making first hires, and so on. Although timing and sequencing advice abounds in the practitioner literature on entrepreneurship, scholarly research to date has yielded unruly and inconclusive results. So much so, in fact, that many researchers have questioned whether reliable temporal regularities exist at all. Our results, however, suggest that such pessimism may be premature. Drawing on data from the Panel Study of Entrepreneurial Dynamics (PSEDII), we examine the incidence of 36 key start-up activities, across a sample of over 960 nascent business firms. We construct multiple indices of temporal similarity among these 36 activities, including patterns of: co-occurrence, substitutability, simultaneity, correlation, and sequentiality. We then subject these similarity indices to a multidimensional scaling (MDS) analysis, in order to construct a map of the start-up "activity space." Results suggest that this space is spanned by two conceptually-plausible dimensions: timeline (early versus late), and modality (operational versus constitutive). Moreover, these underlying dimensions - and the positions of specific start-up activities along these - remain remarkably consistent across various subsets of otherwise distinct ventures. In addition to being of interest in their own right, our findings provide an important first step toward more sophisticated analyses of start-up sequences, holding the potential to advance entrepreneurship research beyond the question of whether start-up stages exist, to more complex and potentially fruitful questions about: (a) the relative similarity and dissimilarity of real-world start-up trajectories; (b) the clustering of trajectories into archetypes, hybrids, and sports; (c) the association of particular archetypes with particular sectors, logics, demographies, and regions; and (d) the performance and survival consequences of conformity to, and deviance from, dominant gestational models. Although there is almost certainly no single "right" way to launch an organization, there may nonetheless be consistent, discernible, theorizable regularities in how new organizations come into being in different contexts. ABSTRACT FROM AUTHOR]; Copyright of Conference Papers -- American Sociological Association is the property of American Sociological Association and its content may not be copied or emailed to multiple sites or posted to a listserv without the copyright holder's express written permission. However, users may print, download, or email articles for individual use. This abstract may be abridged. No warranty is given about the accuracy of the copy. Users should refer to the original published version of the material for the full abstract. (Copyright applies to all Abstracts.)},
author = {Suchman, Mark C and Pollack, Jessica M},
journal = {Conference Papers -- American Sociological Association},
keywords = {BUSINESS models,ECONOMIC activity,ECONOMIC structure,ENTREPRENEURSHIP,FUZZY logic},
pages = {1--19},
title = {{Fuzzy Phases and Multiple Modes: Mapping the Temporal Structure of Entrepreneurial Start-up Activities}},
url = {http://search.ebscohost.com.proxy-ub.rug.nl/login.aspx?direct=true{\&}db=snh{\&}AN=121202607{\&}site=ehost-live{\&}scope=site},
year = {2016}
}
@article{Molenaar2016,
abstract = {Current approaches to model responses and response times to psychometric tests solely focus on between-subject differences in speed and ability. Within subjects, speed and ability are assumed to be constants. Violations of this assumption are generally absorbed in the residual of the model. As a result, within-subject departures from the between-subject speed and ability level remain undetected. These departures may be of interest to the researcher as they reflect differences in the response processes adopted on the items of a test. In this article, we propose a dynamic approach for responses and response times based on hidden Markov modeling to account for within-subject differences in responses and response times. A simulation study is conducted to demonstrate acceptable parameter recovery and acceptable performance of various fit indices in distinguishing between different models. In addition, both a confirmatory and an exploratory application are presented to demonstrate the practical value of the modeling approach.},
author = {Molenaar, Dylan and Oberski, Daniel and Vermunt, Jeroen and {De Boeck}, Paul},
doi = {10.1080/00273171.2016.1192983},
file = {:C$\backslash$:/Users/P280329/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Molenaar et al. - 2016 - Hidden Markov Item Response Theory Models for Responses and Response Times.pdf:pdf},
issn = {00273171},
journal = {Multivariate Behavioral Research},
keywords = {Conditional independence,dynamic modeling,hidden Markov modeling,item response theory,latent class models,response time modeling},
month = {sep},
number = {5},
pages = {606--626},
pmid = {27712114},
publisher = {Routledge},
title = {{Hidden Markov Item Response Theory Models for Responses and Response Times}},
