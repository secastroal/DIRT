title = {{Adaptable User Profiles for Intelligent Geospatial Queries}},
url = {http://search.ebscohost.com.proxy-ub.rug.nl/login.aspx?direct=true{\&}db=snh{\&}AN=18333182{\&}site=ehost-live{\&}scope=site},
volume = {9}
