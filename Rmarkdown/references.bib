pages = {403--415},
pmid = {28375685},
title = {{Explained Variance and Intraclass Correlation in a Two-Level AR(1) Model.}},
url = {https://www.tandfonline.com/doi/full/10.1080/00273171.2017.1299616 http://www.ncbi.nlm.nih.gov/pubmed/28375685},
volume = {52},
