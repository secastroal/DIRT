pages = {186--198},
pmid = {25603359},
title = {{Unraveling affective dysregulation in borderline personality disorder: a theoretical model and empirical evidence.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/abn0000021},
volume = {124},
year = {2015}
}
@article{Steyer1992a,
abstract = {Proposes a solution to the person–situation debate, that argues that not only errors of measurement but also situational effects and effects of the interaction between persons and situations contaminate psychological assessments of traits. The solution is based on a general theory defining states and traits, and consistency, occasion specificity, reliability, and stability coefficients. This theory relies on 2 decompositions: (1) the decomposition of any observed score into a latent state and a measurement error component, and (2) the decomposition of any latent state into a latent trait and a latent state residual. This conceptualizes the view that most psychological attributes have both state and trait components. Several sets of assumptions are presented which lead to different simultaneous equation models. States and traits are simultaneously represented as latent variables in these models.},
