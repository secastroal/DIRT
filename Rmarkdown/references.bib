@article{Moeller2018,
abstract = {This study revisits the structure of emotions by using a co-occurrence network analysis. While previous studies have examined the structure of emotions primarily through interindividual correlations, we investigated how often and which specific positive and negative emotions dccur together within individuals. two studies were conducted with high school students, one (N = 21,678) using restrspective emotion measures (open-ended questions and 28 rated items) and the other (N= 472) using in-the-moment emotion measures (experience sampling). As in previous studies, positive and negative emotion ratins were negatively correlated across individuals, and this negative correlation became stronger when measurement error was controlled. Nevertheless, network analyses of both the open-ended responses and of emotion rating scales found frequent co-occurrences between both positive and negative emotions within individuals and within situations. Across all networks, happy, tired, and stressed were among the most frequent emotions that occurred together with emotions of opposite valence. The network analyses presnted in ths article open new directions to the long-lasting debate about the structure of emotions by reveealing co-occurrences that interindividual correlations would not show.},
author = {Moeller, Julia and Ivcevic, Zorana and Brackett, Marc A. and White, Arielle E.},
doi = {10.1037/emo0000419},
issn = {19311516},
journal = {Emotion},
keywords = {co-occurrence network analysis,experience sampling,intraindividual variation,positive and negative emotions,structure of emotions},
