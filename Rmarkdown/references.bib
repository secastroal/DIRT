keywords = {AFRICAN Americans,Black white crime comparisons,CRIMINAL methods,CRIMINAL statistics,Crime surveys,Crimes against persons,FBI Uniform Crime Reports,Property crimes,Public attitudes toward crime},
title = {{Black Crime {\&} Criminal Victimization (From Criminal Justice system {\&} Blacks, P 37-49, 1984, Daniel Georges-Abeyie, ed. - See NCJ-98968)}},
