pages = {121--155},
publisher = {Springer Fachmedien Wiesbaden},
title = {{State and trait goals of achievement and well-being: structure, validity, and stability}},
url = {http://link.springer.com/10.1007/s11618-015-0667-x},
volume = {19},
year = {2016}
}
