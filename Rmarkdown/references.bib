annote = {Accession Number: SM093400; Kleck, G.; Source Info: 1982; Note: This analysis of homicide rates and gun ownership during the 1947-78 period concludes that the general level of gun ownership has no direct effect on homicide rates, although ownership among violence-prone persons is related to these rates.; Thesaurus Term: GUN control; Subject Term: HOMICIDE; Author-Supplied Keyword: Citizen gun ownership; Author-Supplied Keyword: Crime rate studies; Number of Pages: 37p; Document Type: Research Materials},
author = {Kleck, G},
journal = {Relationship Between Gun Ownership Levels {\&} Rates of Violence in the US (From Handgun Control Legislation - Hearings, P 270-306, 1982 - See NCJ-93399)},
