keywords = {BECK Depression Inventory,CLASSIFICATION,DEPRESSED persons,ENGINEERING -- Study {\&} teaching,FUZZY sets,MEDICAL care costs,MENTAL depression,PSYCHOLOGY -- Research,RASCH models,Rasch model,SCALING (Social sciences),SET theory,SYMBOLIC {\&} mathematical logic,depression,fuzzy partial credit scaling,fuzzy set theory},
number = {9},
