author = {Borhan, Maslan and Jemain, Abdul},
doi = {10.1007/s11205-011-9803-z},
issn = {03038300},
journal = {Social Indicators Research},
keywords = {ALGORITHMS,Belief structure,COMPETITION (Economics),Evidential reasoning,FUZZY sets,MULTIPLE criteria decision making,Multi-criteria decision making,PERFORMANCE,REASONING (Logic),SCHOOL budgets,SCHOOLS},
