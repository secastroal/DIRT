journal = {International Journal of the Sociology of Language},
keywords = {FUZZY logic,GROUP identity,HIGH school students,LANGUAGE {\&} education,METACOGNITION -- Research,MULTILINGUAL education,MULTILINGUALISM,RESEARCH,SOUTH Africa,South African multilingualism,fuzzy languaging logic,language and identity,super-diversity,translanguaging},
number = {234},
pages = {7--26},
title = {{Identity negotiation in a super-diverse community: The fuzzy languaging logic of high school students in Soweto}},
url = {http://search.ebscohost.com.proxy-ub.rug.nl/login.aspx?direct=true{\&}db=snh{\&}AN=103109234{\&}site=ehost-live{\&}scope=site},
