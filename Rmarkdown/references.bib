journal = {JQ: Justice Quarterly},
keywords = {AMERICAN schools abroad,FEAR of crime,FIREARMS owners,KENTUCKY,RESEARCH,SCHOOL violence,SOCIAL sciences,VICTIMS of crimes,weapon possession,“fear and victimization hypothesis{\&}{\#}x201D,“fear and victimization hypothesis”},
