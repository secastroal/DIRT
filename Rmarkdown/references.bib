title = {{Psychometric properties of a computerized mobile phone method for assessing mood in daily life.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0017813 http://www.ncbi.nlm.nih.gov/pubmed/20141308},
