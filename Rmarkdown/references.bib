author = {Bouwmeester, Samantha and Sijtsma, Klaas},
doi = {10.1080/00273170701384324},
issn = {0027-3171; 1532-7906},
journal = {Multivariate Behavioral Research},
keywords = {2007,Binomial Distribution,Fuzzy Set Theory,Item Response Theory,Reasoning,Task Complexity,binomial mixture model,development phases,fuzzy trace theory,latent class modeling,transitive reasoning},
number = {3},
